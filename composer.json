{
    "name": "nazonohito51/typed-properties-cconverter",
    "description": "convert @ver in phpdoc to PHP7.4 typed properties",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "nazonohito51",
            "email": "nazonohito51@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "nikic/php-parser": "^4.3",
        "phpdocumentor/reflection-docblock": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "TypedPropertiesConverter\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "bin": ["bin/typed-properties-converter"]
}
